{
  "name": "rt-notify",
  "version": "1.3.0",
  "description": "simple yet powerful notifications for React",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "author": "Glebcha",
  "license": "MIT",
  "repository": "https://github.com/glebcha/rt-notify",
  "files": [
    "build",
    "yarn.lock"
  ],
  "keywords": [
    "react",
    "notification",
    "toast",
    "rt-notify"
  ],
  "scripts": {
    "start": "start-storybook --port 9444",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "stylelint": "stylelint \"src/**/*.css\"",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "build": "cross-env NODE_ENV=production rollup -c && npm run changelog",
    "changelog": "changelog/generate-changelog",
    "preversion": "npm run lint",
    "postversion": "npm run build",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --extends=@commitlint/config-conventional -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm run stylelint"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
